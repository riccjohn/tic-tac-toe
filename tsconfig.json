{
  "compilerOptions": {
    "lib": ["es5", "es6", "dom"],
    "moduleResolution": "node",
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "jsx": "react",
    "allowJs": true,
    "target": "es5",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "./*/__tests__"],
  "include": ["./src/"]
}
